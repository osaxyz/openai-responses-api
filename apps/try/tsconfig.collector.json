{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "outDir": "./dist/collector"
    },
    "include": ["src/collector/**/*.ts", "src/constants/**/*.ts", "src/types/**/*.ts"]
}
